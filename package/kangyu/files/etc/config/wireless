config wifi-device 'radio0'
	option type 'mac80211'
	option phy 'phy0'
	option hwmode '11ng'
	list ht_capab 'SHORT-GI-40'
	list ht_capab 'DSSS_CCK-40'
	option htmode 'HT40-'
	option channel '11'
	option noscan '1'
	option country 'US'
	option disable '0'
	option txpower '27'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option ifname 'wlan0'
	option mode 'ap'
	option ssid 'KangYu-WPA 康语'
	option key 'dragino-dragino'
	option encryption 'psk2'
	option maxassoc '30'

config wifi-iface
	option device 'radio0'
	option network 'hotspot'
	option ifname 'wlan1'
	option mode 'ap'
	option ssid 'KangYu-OPEN 康语'
	option encryption 'none'
	option maxassoc '30'
